{
    "name": "firephp",
    "engine": [
        "node"
    ],
    "main": "lib/project.js",
    "scripts": {
        "import": {
            "location": "./",
            "module": "/scripts/import.js"
        },
        "build": {
            "location": "./",
            "module": "/scripts/build.js"
        },
        "publish": {
            "location": "./",
            "module": "/scripts/publish.js"
        }
    },
    "dependencies": [
        {
            "id": "github.com/firephp/firephp/"
        }
    ],
    "mappings": {
        "nodejs": {
            "id": "nodejs.org"
        },
        "pinf": {
            "id": "pinf.org/loader"
        },
        "modules": {
            "id": "github.com/pinf/modules-js/"
        }
    }
}